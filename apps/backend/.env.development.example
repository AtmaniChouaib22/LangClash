# Development Environment Variables Template
# Copy this file to .env.development and fill in your actual values

NODE_ENV=development

# App Configuration
PORT=3001
LOG_LEVEL=debug

# Database (Docker PostgreSQL for development)
# DATABASE_URL="postgresql://username:password@localhost:5433/database_name"
# DIRECT_URL="postgresql://username:password@localhost:5433/database_name"

# Redis (Docker Redis for development)
# REDIS_URL="redis://localhost:6379"

# Supabase (Development instance)
# SUPABASE_URL="https://your-project.supabase.co"
# SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# JWT Secret (Development - use a simple secret)
# JWT_SECRET="your-dev-jwt-secret"

# CORS Configuration
CORS_ORIGIN="http://localhost:3000"

# Development Features
ENABLE_DEBUG_LOGS=true
ENABLE_CORS=true